<!DOCTYPE html>
<html>
  <head>
    <title>Lista de Usuário</title>
  </head>
  <body>
    <h1><%= mensagem %></h1>
    <ul>
      <% docs.forEach(function(bilheteunico){ %>
      <li>
        <h2><%= usuario.nome %></h2>
        <input
          type="button"
          onclick="location.href='/delete/bilheteunico/<%= bilheteunico.id %>'"
          value="delete"
        />
        <input
          type="button"
          onclick="location.href='/edit/bilheteunico/<%= bilheteunico.id %>'"
          value="editar"
        />
      </li>
      <% }) %>
    </ul>
    <br />
    <br />
    <input
      type="button"
      onclick="location.href='/formulario_bilhete_unico'"
      value="Novo usuário"
    />
  </body>
</html>
